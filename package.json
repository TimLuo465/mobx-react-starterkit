    "typescript": "^2.8.1",
    "typings-for-css-modules-loader": "^1.7.0",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "mobx": "^4.2.0",
    "mobx-react": "^5.0.0",
    "mockjs": "^1.0.1-beta3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2"
  }
